<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LicenseFragment">

    <View
        android:id="@+id/back_to_others_view_from_license_view_background_view"
        android:layout_width="108dp"
        android:layout_height="48dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <ImageView
        android:id="@+id/back_to_others_view_from_license_view_image_view"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        />

    <TextView
        android:id="@+id/back_to_others_view_from_license_view_text_view"
        android:text="その他"
        android:textSize="16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/back_to_others_view_from_license_view_image_view"
        android:layout_marginTop="16dp"
        android:layout_marginStart="8dp"
        />

    <TextView
        android:id="@+id/license_top_bar"
        android:text="ライセンス"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        />

    <View
        android:id="@+id/supported_by_background_view"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        app:layout_constraintTop_toBottomOf="@+id/license_top_bar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="32dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/supported_by_text_view"
        android:text="Supported by Rakuten Developers"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/supported_by_background_view"
        app:layout_constraintLeft_toLeftOf="@+id/supported_by_background_view"
        app:layout_constraintBottom_toBottomOf="@+id/supported_by_background_view"
        android:layout_marginStart="16dp"
        />

    <ImageView
        android:id="@+id/supported_by_image_view"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="@+id/supported_by_background_view"
        app:layout_constraintRight_toRightOf="@+id/supported_by_background_view"
        app:layout_constraintBottom_toBottomOf="@+id/supported_by_background_view"
        android:layout_marginEnd="16dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>