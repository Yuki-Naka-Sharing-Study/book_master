<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout6"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AboutFutureDevelopmentFragment">

    <View
        android:id="@+id/back_to_others_view_from_about_future_development_view_background_view"
        android:layout_width="108dp"
        android:layout_height="48dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <ImageView
        android:id="@+id/back_to_others_view_from_about_future_development_view_image_view"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        />

    <TextView
        android:id="@+id/back_to_others_view_from_about_future_development_view_text_view"
        android:text="その他"
        android:textSize="16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/back_to_others_view_from_about_future_development_view_image_view"
        android:layout_marginTop="16dp"
        android:layout_marginStart="8dp"
        />

    <TextView
        android:id="@+id/license_top_bar"
        android:text="これからの開発について"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        />

    <View
        android:id="@+id/about_future_dev_share_background_view"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        app:layout_constraintTop_toBottomOf="@+id/license_top_bar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="28dp"
        android:layout_marginHorizontal="28dp"
        />

    <TextView
        android:id="@+id/about_future_dev_description"
        android:text="@string/about_future_dev_description"
        android:layout_width="match_parent"
        android:layout_height="248dp"
        app:layout_constraintTop_toBottomOf="@+id/about_future_dev_share_background_view"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="44dp"
        android:layout_marginHorizontal="16dp"
        />

    <View
        android:id="@+id/contanct_form_background_view"
        android:layout_width="216dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/about_future_dev_description"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="20dp"
        />

    <TextView
        android:id="@+id/contact_form_text_view"
        android:text="お問合わせフォーム"
        android:textSize="16sp"
        android:background=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/contanct_form_background_view"
        app:layout_constraintLeft_toLeftOf="@+id/contanct_form_background_view"
        app:layout_constraintRight_toLeftOf="@+id/contanct_form_image_view"
        app:layout_constraintBottom_toBottomOf="@+id/contanct_form_background_view"
        android:layout_marginStart="16dp"
        />

    <ImageView
        android:id="@+id/contanct_form_image_view"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="@+id/contanct_form_background_view"
        app:layout_constraintLeft_toRightOf="@+id/contact_form_text_view"
        app:layout_constraintBottom_toBottomOf="@+id/contanct_form_background_view"
        android:layout_marginStart="16dp"
        />

    <View
        android:id="@+id/review_background_view"
        android:layout_width="132dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/contanct_form_background_view"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="24dp"
        />

    <TextView
        android:id="@+id/eview_text_view"
        android:text="レビューする"
        android:textSize="16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/review_background_view"
        app:layout_constraintLeft_toLeftOf="@+id/review_background_view"
        app:layout_constraintRight_toRightOf="@+id/review_background_view"
        app:layout_constraintBottom_toBottomOf="@+id/review_background_view"
        />

</androidx.constraintlayout.widget.ConstraintLayout>